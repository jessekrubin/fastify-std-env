{
  "extends": "@jsse/tsconfig",
  "compilerOptions": {
    "jsx": "react",
    "module": "node16",
    "moduleResolution": "node16",
    "outDir": "dist",
    "skipLibCheck": true
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts",
    "*.ts"
  ],
  "exclude": [
    "./fixtures/**/*.*",
    "./dist/**/*.*",
    "./node_modules/**/*.*"
  ],
  "compileOnSave": false
}